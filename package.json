{
  "name": "dui-nuxt",
  "version": "1.0.3",
  "description": "Tailwind components for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run minify && npm run dev:prepare && npm run dev",
    "minify": "tailwindcss -i ./src/runtime/assets/css/main.css -o ./src/runtime/assets/css/output.css --minify",
    "build-css": "tailwindcss -i src/runtime/assets/css/main.css -o ./dist/output.css --minify",
    "prepack": "nuxt-module-build build && tailwindcss -i src/runtime/assets/css/main.css -o ./dist/output.css --minify",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.1",
    "@nuxtjs/tailwindcss": "^6.13.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.1",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.23.0",
    "nuxt": "^3.16.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.8.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
